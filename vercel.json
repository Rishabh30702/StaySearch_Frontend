{
  "version": 2,
  "builds": [
    {
      "src": "angular.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/stay-search-frontend"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/stay-search-frontend/index.html"
    }
  ]
}
